OPTIONS_SERVER+=-logging logging.properties
OPTIONS_SERVER+=-export config.xml
OPTIONS_SERVER+=-import config.xml


OPTIONS_LOGGER:=-topics
OPTIONS_LOGGER+= MY,
OPTIONS_LOGGER+= ALARM,
OPTIONS_LOGGER+= CONFIGS

OPTIONS_CONFIG_LOGGER:=


